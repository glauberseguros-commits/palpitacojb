{
  "version": 2,
  "buildCommand": null,
  "installCommand": null,
  "outputDirectory": "build",
  "routes": [
    { "src": "^/health$", "dest": "https://palpitacojb.onrender.com/health" },
    { "src": "^/api/(.*)$", "dest": "https://palpitacojb.onrender.com/api/$1" },

    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}